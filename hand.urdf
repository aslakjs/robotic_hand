    <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
    <!-- Hand: -->
    <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
    <joint name="wrist" type="revolute">
        <parent link="Forearm"/>
        <child link="Palm"/>
        <limit effort="1000.0" lower="-30" upper="0" velocity="0.5"/>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <axis xyz="0 1 0" />
    </joint>
    <joint name="F1" type="revolute">
