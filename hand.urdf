    <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
    <!-- Arm: -->
    <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
    <link name="Forearm"> 
        <inertial>
            <origin xyz = "0 0 0" />
            <mass value = "1" />
            <inertia ixx = "0.5" iyy = "0.5" izz = "0.5" ixy = "0" ixz = "0" iyz = "0" />
        </inertial>
        <visual>
            <origin xyz = "0 0 0" />
            <geometry>
                <cylinder radius = "1" length = "1"  />
            </geometry>
            <material name = "gray 1">
                <color rgba = "0 0 0 1" />
            </material>
        </visual>
	</link>


